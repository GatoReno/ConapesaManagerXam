<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls="clr-namespace:XLabs.Forms.Controls;assembly=XLabs.Forms"
             x:Class="Conapesca_Manager.PageNav">
    <ContentPage.ToolbarItems
        >
        <ToolbarItem
            Text="Solicitudes"
         Activated="ToolbarItem_Solicitudes"
            Icon="list.png"
            x:Name="Solic"
            />
        <ToolbarItem
            Text="Instrucciones"
            Activated="ToolbarItem_Instrucciones"
            Icon="gear.png"
            x:Name="Insturc"
            />
        <ToolbarItem
            Text="User"
            Activated="ToolbarItem_InfoUser"
            Icon="user.png"
            x:Name="Userinfo"
            />

        <!--<ToolbarItem
            Text="User"
            Activated="ToolbarItem_Sign"
            Icon="sign.png"
            />-->

    </ContentPage.ToolbarItems>



    <ContentPage.Content>
        <StackLayout
            Margin="0,10,0,0">
            
            <Label
                x:Name="Mainlbl"
                HorizontalOptions="CenterAndExpand"
                Text="Bienvenido"/>
           

        </StackLayout>
    </ContentPage.Content>

   
</ContentPage>